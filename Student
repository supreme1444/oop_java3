package Iter;

import java.util.Iterator;

public class ReverseIterator implements Iterator<Student> {
    private int index = 3;
    private Studentgroup studentgroup1;

    public ReverseIterator(Studentgroup studentgroup1) {
        this.studentgroup1 = studentgroup1;
    }

    @Override
    public boolean hasNext() {
       return index>studentgroup1.getSize()-3;
       
       
    }

    @Override
    public Student next() {

        return studentgroup1.get(--index);
    }
}
